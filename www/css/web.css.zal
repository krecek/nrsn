.clanek {font-family:arial; font-size:12px; padding: 0 3em 0 0;}
h1, h2, h3, h4 {text-transform:uppercase;}
h2 {font-size:16px;font-family:Open Sans, Arial, Sans-Serif;}
h1 {font-size:22px;font-family:Open Sans, Arial, Sans-Serif; margin-top:0;}
h2.podbarvit {border-top: 1px solid #cf6370; padding: 0.3em;}
h1.podbarvit {background:url("tecky1.png") repeat-x scroll left -398px rgba(0, 0, 0, 0); padding:0.3em;}
.pg-menu li {width:auto; float:none;}
.pg-menu li li, .pg-menu li li a {padding:0;}
.telo {
    margin-left: auto;
    margin-right: auto;
    padding-left: 15px;
    padding-right: 15px;
    background: white; /*rgb(204,204,102);*/
    max-width: 1090px;
}
.radek {
    margin-left: -15px;
    margin-right: -15px;
    content: " ";
    display: block;
}
.box-1, .box-2, .box-3, .box-4, .box-5, .box-6, .box-7, .box-8, .box-9, .box-10, .box-11, .box-12, .box-8-menu {
    float: left;     
    min-height: 1px;
    padding-left: 15px;
    padding-right: 15px;
    position: relative;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.box-12 { width: 100%;}
.box-11 { width: 91.6667%;}
.box-10 { width: 83.3333%;}
.box-9 { width: 75%;}
.box-8 { width: 66.6667%; }
.box-7 { width: 58.3333%;}
.box-6 { width: 50%;}
.box-5 { width: 41.6667%; }
.box-4 { width: 33.3333%;}
.box-3 { width: 25%;}
.box-2 { width: 16.6667%; }
.box-1 { width: 8.33333%;}
.box-8-menu {width:12.5%;}
*:before, *:after {
    -moz-box-sizing: border-box;
        box-sizing: border-box;
}
.radek:after, .radek:before {clear:both;
    content: " ";
    display: block;}
.top {background:white; color:black; line-height: 60px; margin: 0 0 20em;box-shadow: 0 0 15px #666666; position:static;}
.top a {color:#3399ff; text-decoration:none; text-transform:uppercase;}
.top .telo {background:transparent;}
.logo {height:60px; width:auto; float:left;}
.right {text-align:right;}
ul.naradek {padding:0; margin:0;}
.naradek li {display:inline-block; font-family:arial; font-size:12px; padding:0; margin:0; list-style-type:none;}
.naradek li a {display:inline-block;padding:0 10px; border-left: 1px solid #ccc; height:12px; line-height: 12px;}
.naradek li a { border-color:#fff;}
.naradek li + li a {border-color:#3399ff;}
.naradek.blok li {display:block; margin: 5px 0 0 0;}
.naradek.blok li a {border-color:#3399ff; padding: 6px 10px; text-decoration:none; color:#3399ff; height:auto;}
.naradek.blok li.active a {border-width: 5px; padding-left:5px; font-weight:bold;}
.naradek.blok li.active a:hover {padding-left:10px;border-width: 1px;}
.naradek li a:hover {background:#3399ff; color:white; padding:6px 10px;border-radius: 0.3em;}
.logo {border:0;}
.menu-sporty {display:block;}
.nadpis {font-family: verdana; font-size: 20px;}
.nadpis.rubrika {position:absolute; top: -4.5em; left:0; display:block; font-size:40px; font-weight:normal; font-family: Open Sans, Arial; color:white; 
padding:  0.2em 0.5em; background-color: rgba(215, 0, 25, 0.5); /* modra background-color:rgba(51, 153, 255, 0.7);  zelena rgba(159, 190, 60, 0.5); */}
.obsah {box-shadow: 1px 1px 15px #666666;}
.mini {padding:10px 0 20px 0;}
.mini a {font-size:10px;}
form.search {position:relative;padding: 10px 50px 10px 10px; text-align:right; background:#e5e5e5; border-bottom-left-radius: 0.3em; border-bottom-right-radius: 0.3em;}
form.search input {padding:3px 5px; border-radius: 0.3em; border: 1px solid #e5e5e5;display:block; width:100%; line-height:2em;height:2em;}
form.search input[type="submit"] {
    font-family: 'gis';
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    line-height: 1;
    text-transform: none;max-width:30px; border:0;background:#3399ff; color:white; cursor:pointer;position:absolute; right:10px;top:10px;}
.m-con table {width:100%;background:#f0f0f0;border-radius: 0.3em;padding:0.3em;}
.m-con td, .m-con th {width:14.28571%; text-align:center; line-height: 1.5em;}
.m-con td {border:1px solid #f0f0f0;background:#ffffff;} 
.m-con td.weekend {background:#ffffcc;}
.m-con a {text-decoration:none;}
.m-con td a {display: block; height:100%; width:100%; background:#3399ff; color:white;}
.m-con .day-d {border:1px solid #cf6370;}
.soc {    font-family: 'gis';
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    line-height: 1;
    font-size:28px;
    padding:20px 0;}
    .soc a {color:silver;text-decoration:none;}
    .soc a.rss:hover {color:rgb(255,153,0)}
    .soc a.fb:hover {color:#3b5998;}
    .soc a.gp:hover {color:#dd4b39;}
    .soc a.yt:hover {color:#dd4b39;}
.spodnilista {background: #4d4d4d; color:white; padding:15px 0;}
.spodnilista a {color:#c0c0c0; text-decoration:none;}
.spodnilista a:hover {color:white;}
.spodnilista ul {padding-left:15px;}
.radek.spodnilista {margin:0;}
/* kulate menu */
.menu-sporty {
    background: none repeat scroll 0 0 #FFFFFF;
    box-shadow: 1px 1px 15px #808080;
    display: none;
    height: 17em;
    padding: 3em 1em 0;
    margin: 0;
    max-width: 1095px;
    position: absolute;
    top: 45px;
}
.menu-sporty li a {

    border-radius: 50em;
    box-shadow: 1px 1px 10px #000000;
    display: block;
    height: 40px;
    overflow: visible;
    padding: 90px 0 0;
    text-decoration: none;
    margin: 5px -10px;    
}
.menu-sporty li a span {
    background: none repeat scroll 0 0 #3399FF;
    border-radius: 0.3em;
    bottom: -20px;
    color: #FFFFFF;
    display: block;
    padding: 0.5em;
    position: relative;
    text-align: center;
    text-overflow: ellipsis;
}
